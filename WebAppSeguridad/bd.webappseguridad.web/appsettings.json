{

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning"
    }
  },

  //Nombre del servicio de log que tiene que ser igual al nombre que tiene este en la base de datos
  "ServiciosLog": "LogWebService",
  "HostServicioLog": "http://developmentds.eastus.cloudapp.azure.com:81/LOG",
  //Host de servicio de seguridad tiene que ser la dirección
  //dode se encuentra el servicio de seguridad
  //"HostServicioSeguridad": "http://uiowtfundation3/swSeguridad",
  "HostServicioSeguridad": "http://developmentds.eastus.cloudapp.azure.com:81/SEGURIDAD",
  //Nombre de la aplicación este es con el nombre que se guardará en los Log
  "NombreAplicacion": "MVCSEGUR",

  //Días válido de la clave de encriptacióndor
  "DiasValidosClaveEncriptada": 10,
  //Host de donde se encuentra el sitio de Login.
  "HostLogin": "http://localhost:85",
  //"HostLogin": "http://localhost:62905",

  //Configuración del tiempo de vida de la cookie Horas-Minutos-Segundos
  "TiempoVidaCookieHoras": 0,
  "TiempoVidaCookieMinutos": 0,
  "TiempoVidaCookieSegundos": 0,


  ///----------Niveles del menú------------------

  "NivelMenu": "6"

  ///---------Fin Niveles Menú--------------------





